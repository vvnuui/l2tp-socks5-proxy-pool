# 环境配置示例
# 复制此文件为 .env 并修改配置

# Django 配置
DEBUG=False
SECRET_KEY=your-secret-key-change-me-in-production
ALLOWED_HOSTS=localhost,127.0.0.1,your-domain.com

# 数据库配置
DB_NAME=socks_proxy
DB_USER=postgres
DB_PASSWORD=your-secure-password

# CORS 配置
CORS_ALLOWED_ORIGINS=http://localhost,https://your-domain.com

# 代理池配置
PROXY_IP_POOL_START=10.0.0.2
PROXY_IP_POOL_END=10.0.3.254
PROXY_PORT_START=10800
PROXY_PORT_END=11900
PROXY_LOCAL_IP=10.0.0.1

# PPP 钩子 Token
PPP_HOOK_TOKEN=your-secret-token-change-me

# IPSec PSK
IPSEC_PSK=YourSharedSecretHere
